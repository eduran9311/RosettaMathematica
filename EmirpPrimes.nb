Cell[BoxData[
 RowBox[{
  RowBox[{"FindNEmirps", "[", "n_Integer", "]"}], ":=", 
  RowBox[{
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"i", "=", "0"}], ",", 
       RowBox[{"prime", "=", "2"}]}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{"Reap", "[", "\[IndentingNewLine]", 
      RowBox[{"While", "[", 
       RowBox[{
        RowBox[{"i", "<", "n"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"#1", "!=", "#2"}], "&&", 
              RowBox[{"PrimeQ", "[", "#2", "]"}]}], "\[IndentingNewLine]", 
             ",", 
             RowBox[{
              RowBox[{"Sow", "[", "#1", "]"}], ";", 
              RowBox[{"i", "++"}]}]}], "\[IndentingNewLine]", "]"}], "&"}], "@@", 
          RowBox[{"{", 
           RowBox[{"prime", ",", 
            RowBox[{"IntegerReverse", "[", "prime", "]"}]}], "}"}]}], 
         "\[IndentingNewLine]", ";", 
         RowBox[{"prime", "=", 
          RowBox[{"NextPrime", "[", "prime", "]"}]}]}]}], 
       "\[IndentingNewLine]", "]"}], "\[IndentingNewLine]", "]"}]}], "]"}], 
   "[", 
   RowBox[{"[", 
    RowBox[{"2", ",", "1"}], "]"}], "]"}]}]], "Input",
 CellChangeTimes->{{3.8366086801453743`*^9, 3.836608722380145*^9}, {
   3.83660876933932*^9, 3.836608792154795*^9}, 3.836608861825816*^9, {
